<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/zch_checklist_logos2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/zch_checklist_logos2"
        android:supportsRtl="true"
        android:theme="@style/Theme.RealwearUUUHHHHHH"
        tools:targetApi="31">
        <activity
            android:name=".Tools"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Main"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="checklick" android:host="auth" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".SelectTool" />
        <activity android:name=".CheckList" />
        <activity android:name=".CheckList2" />
        <activity android:name=".CheckList3" />
        <activity android:name=".CheckList4" />
        <activity android:name=".CheckList5" />
        <activity android:name=".CheckList6" />
        <activity android:name=".CheckList7" />
        <activity android:name=".Finish" />
        <activity android:name=".PDF" />
        <activity android:name=".PDFDownload" />
        <activity android:name=".EmployeeList" />
        <activity android:name=".FileUpload" />
    </application>

</manifest>
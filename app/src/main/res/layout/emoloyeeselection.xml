<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="90dp"
        android:background="@drawable/reactangle" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="311dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="321dp"
        android:text="Technician Selection"
        android:textColor="#000000"
        android:textSize="24sp" />

    <EditText
        android:id="@+id/InputEmNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="123dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="521dp"
        android:ems="10"
        android:hint="Employee No."
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/NameEmployee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="123dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="521dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/inputCompany"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="483dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="161dp"
        android:ems="10"
        android:hint="Company Name"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/inputInitials"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="485dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="159dp"
        android:ems="10"
        android:hint="Initials"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/backToTools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/InputEmNumber"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="85dp"
        android:layout_marginTop="83dp"
        android:layout_marginEnd="681dp"
        android:text="Back" />

    <Button
        android:id="@+id/startPDF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/InputEmNumber"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="679dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="87dp"
        android:text="Start" />

</RelativeLayout>